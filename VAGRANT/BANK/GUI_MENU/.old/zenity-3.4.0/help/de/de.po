"if [ \"$?\" = -1 ] ; then\n"
"        zenity --error \\\n"
"          --text=\"Update canceled.\"\n"
"fi\n"
msgstr ""
"\n"
"        #!/bin/sh\n"
"        (\n"
"        echo \"10\" ; sleep 1\n"
"        echo \"# Mail-Logdateien werden aktualisiert\" ; sleep 1\n"
"        echo \"20\" ; sleep 1\n"
"        echo \"# Cronjobs werden zurückgesetzt\" ; sleep 1\n"
"        echo \"50\" ; sleep 1\n"
"        echo \"Diese Zeile wird ignoriert\" ; sleep 1\n"
"        echo \"75\" ; sleep 1\n"
"        echo \"# Neustart des Systems\" ; sleep 1\n"
"        echo \"100\" ; sleep 1\n"
