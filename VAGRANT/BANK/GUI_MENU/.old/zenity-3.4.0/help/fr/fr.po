"\n"
"#!/bin/sh\n"
"\n"
"COLOR=`zenity --color-selection --show-palette`\n"
"\n"
"case $? in\n"
"         0)\n"
"\t\techo \"You selected $COLOR.\";;\n"
"         1)\n"
"                echo \"No color selected.\";;\n"
"        -1)\n"
"                echo \"An unexpected error has occurred.\";;\n"
"esac\n"
