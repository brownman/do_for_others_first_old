"         0)\n"
"\t\techo \"You selected $VALUE%.\";;\n"
"         1)\n"
"                echo \"No value selected.\";;\n"
"        -1)\n"
"                echo \"An unexpected error has occurred.\";;\n"
"esac\n"
msgstr ""
"\n"
"#!/bin/sh\n"
"\n"
"VALUE=`zenity --scale --text=\"ウィンドウの透過率を指定してください。\" --value=50`\n"
"\n"
"case $? in\n"
"         0)\n"
"\t\techo \"$VALUE% が指定されました。\";;\n"
"         1)\n"
"                echo \"値が指定されませんでした。\";;\n"
"        -1)\n"
"                echo \"予期せぬエラーが発生しました。\";;\n"
"esac\n"

