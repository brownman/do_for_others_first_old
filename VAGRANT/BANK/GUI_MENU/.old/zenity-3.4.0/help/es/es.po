"\n"
"case $? in\n"
"         0)\n"
"\t \techo \"Nombre de usuario: `echo $ENTRY | cut -d'|' -f1`\"\n"
"\t \techo \"Contraseña: `echo $ENTRY | cut -d'|' -f2`\"\n"
"\t\t;;\n"
"         1)\n"
"                echo \"No iniciar sesión.\";;\n"
"        -1)\n"
"                echo \"Ha ocurrido un error inesperado.\";;\n"
"esac\n"

