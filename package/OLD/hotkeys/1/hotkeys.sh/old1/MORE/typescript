    [1m[37mfi
else[m
    [1m[33mecho[m[1m[35m [m[1m[31m'[m[1m[35msupply a script to wrap[m[1m[31m'[m
    [1m[33mexit[m
[1m[37mfi


blue()       {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;30m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}
red() {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;31m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}
green()         {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;32m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}

print_error(){[m
    red [1m[31m"[m[1m[35mprint error:[m[1m[31m"[m
    [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[34m$@[m[1m[31m"[m
[1m[37m}
str_to_arr(){[m
    [36m#depend on: arr[m
    blue [1m[31m'[m[1m[35mstr_to_arr[m[1m[31m'[m
    [1m[33mlocal[m [1m[36mstr[m=[1m[31m"[m[1m[34m$1[m[1m[31m"[m
    [1m[33mlocal[m [1m[36mdelimeter[m=[1m[34m${2[m[1m[31m-[m:[1m[34m}[m
    [1m[34mIFS[m[1m[31m=[m[1m[34m$delimeter[m [1m[33mread[m [35m-a[m arr [1m[31m<<<[m [1m[31m"[m[1m[34m$str[m[1m[31m"[m
    [36m#result: arr[m
[1m[37m}


use_error(){[m
    blue [1m[31m'[m[1m[35muse_error()[m[1m[31m'[m
    [1m[33mlocal[m [1m[36mline[m=[1m[31m"[m[1m[34m$1[m[1m[31m"[m
    [1m[36marr[m=()[35;1H[K[34;61H[1m[7m38[9;1H[?12l[?25h[?25l[2;33r[m[2;1H[30M[1;36r[4;4H [1m[33mlocal[m [1m[36mstr[m=[35m`echo [m[1m[34m$line[m[35m [m[1m[31m|[m[35m [m[1m[33msed[m[35m [m[1m[31m'[m[1m[35ms/line /+/g[m[1m[31m'[m[35m`[m
    [36m#    echo "$str"[m
    str_to_arr [1m[31m"[m[1m[34m$str[m[1m[31m"[m
    [36m#we have: arr[m
    [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mARR[m[1m[31m"[m[10;4H [1m[36mexe[m=[1m[31m"[m[1m[34m${arr[[m[1m[35m0[m[1m[34m]}[m[1m[31m"[m
    [1m[36mline[m=[1m[31m"[m[1m[34m${arr[[m[1m[35m1[m[1m[34m]}[m[1m[31m"[m
    [1m[36merror[m=[1m[31m"[m[1m[34m${arr[[m[1m[35m2[m[1m[34m]}[m[1m[31m"[m
    [1m[36merror_code[m=[1m[31m"[m[1m[34m${arr[[m[1m[35m3[m[1m[34m]}[m[1m[31m"[m[17;4H [36m#IFS=\s[m
    [36m#echo "${arr[@]}" > /tmp/error.txt[m
    notify-send [1m[31m"[m[1m[34m$error[m[1m[31m"[m [1m[31m"[m[1m[34m$error_code[m[1m[31m"[m
    [1m[33mprintf[m [1m[31m"[m[1m[35m%s[m[35m\n[m[1m[31m"[m [1m[31m"[m[1m[34m${arr[[m@[1m[34m]}[m[1m[31m"[m [1m[31m>[m /tmp/error.txt
    cat [35m-n[m /tmp/error.txt[23;4H [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mvi [m[1m[34m$exe[m[1m[35m [m[1m[34m$line[m[1m[31m"[m[1m[35m [m[1m[31m|[m xsel [35m--clipboard[m
    [1m[33msleep[m [1m[35m1[m
    green [1m[31m'[m[1m[35myour clipboard has been updated![m[1m[31m'[m
[1m[37m}
try(){[m
    blue [1m[31m'[m[1m[35mtry()[m[1m[31m'[m
    [36m#echo something >> $file_logger[m
    [36m#    $( exec "$script" 2>&1 1>$file_logger )[m
    [1m[33meval[m [1m[34m$script[m [1m[35m2[m[1m[31m>[m[1m[34m$file_logger[m
    [1m[33mlocal[m [1m[36merror_code[m=[1m[34m$?[m
    [1m[37mif [m[1m[31m[[m [1m[31m"[m[1m[34m$error_code[m[1m[31m"[m [1m[37m-eq[m [1m[35m0[m   [1m[31m];[m[1m[37mthen[m[34;61H[1m[7m68[9;1H[?12l[?25h[?25l[2;33r[m[2;1H[30M[1;36r[4;8H green [1m[31m"[m[1m[35mno error_code[m[1m[31m"[m
    [1m[37melse[m[6;8H red [1m[31m"[m[1m[35merror_code: [m[1m[34m$error_code[m[1m[31m"[m
    [1m[37mfi

}
show_log(){[m
    blue  [1m[31m'[m[1m[35mshow_log()[m[1m[31m'[m [36m#[m
    [36m#    exit[m
    [36m#    cmd="$script ${args[@]}" [m
    [36m#    echo "cmd: $cmd"[m[16;4H [36m#    $(eval "$cmd") 2> $file_logger [m
    [36m#[m
    [1m[37mif [m[1m[31m[[m [1m[37m-f[m [1m[31m"[m[1m[34m$file_logger[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[19;8H [1m[37mif [m[1m[31m[[m [1m[37m-s[m [1m[31m"[m[1m[34m$file_logger[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[20;12H green [1m[31m"[m[1m[35mlogger is empty[m[1m[31m"[m[21;12H [1m[36mline[m=[35m`cat [m[1m[34m$file_logger[m[35m`[m[22;12H print_error [1m[31m"[m[1m[34m$line[m[1m[31m"[m[23;12H use_error [1m[31m"[m[1m[34m$line[m[1m[31m"[m[24;12H [36m#   use_error "$line"[m[25;8H [1m[37melse[m[26;12H red [1m[31m"[m[1m[35mlogger is not empty[m[1m[31m"[m[27;12H blue [1m[31m"[m[1m[34m$file_logger[m[1m[35m:[m[1m[31m"[m[28;12H cat [1m[34m$file_logger[m[29;12H [1m[33mecho[m[30;12H [1m[33mecho[m[1m[35m Press Enter![m[31;12H [1m[33mread[m[32;8H [1m[37mfi[m
    [1m[37melse[m[34;61H[1m[7m98[9;1H[?12l[?25h[?25l[2;33r[m[2;1H[30M[1;36r[4;8H red [1m[31m'[m[1m[35mno such file[m[1m[31m'[m
    [1m[37mfi

}

steps(){[m
    blue [1m[31m"[m[1m[35mrun()[m[1m[31m"[m
    green [1m[31m"[m[1m[35msteps: 1.try   2.show_log[m[1m[31m"[m
    try
    show_log

[1m[37m}
breakpoint(){[m
    [1m[33mecho[m[1m[35m breakpoint![m
[1m[37m}[m
[36m#native "logger: $file_logger"[m
steps
[1m[33mtrap[m breakpoint ERR
[1m[34m~                                                                                                                             [23;1H~                                                                                                                             [24;1H~                                                                                                                             [25;1H~                                                                                                                             [26;1H~                                                                                                                             [27;1H~                                                                                                                             [28;1H~                                                                                                                             [29;1H~                                                                                                                             [30;1H~                                                                                                                             [31;1H~                                                                                                                             [32;1H~                                                                                                                             [33;1H~                                                                                                                             [m[34;61H[1m[7m128[9;1H[?12l[?25h[?25l[m[36;1H[1m-- VISUAL --[9;1H[?12l[?25h[?25l[m[1m[7m[37msteps(){[m[7m [m[34;63H[1m[7m9 [10;1H[?12l[?25h[?25l[m[7m    blue [m[1m[7m[31m"[m[1m[7m[35mrun()[m[1m[7m[31m"[m[7m [m[34;62H[1m[7m30 [11;1H[?12l[?25h[?25l[m[7m    green [m[1m[7m[31m"[m[1m[7m[35msteps: 1.try   2.show_log[m[1m[7m[31m"[m[7m [