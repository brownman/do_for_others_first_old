    [36m#    exit[m[22;70H[1m[7m60 [9;5H[?12l[?25h[?25l[2;21r[m[2;1H[18M[1;24r[4;4H [36m#    cmd="$script ${args[@]}" [m
    [36m#    echo "cmd: $cmd"[m
    [36m#    echo -n '' > $file_logger[m
    [36m#    $(eval "$cmd") 2> $file_logger [m
    [36m#[m
    [1m[37mif [m[1m[31m[[m [1m[37m-f[m [1m[31m"[m[1m[34m$file_logger[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[10;8H [1m[37mif [m[1m[31m[[m [1m[37m![m [1m[37m-s[m [1m[31m"[m[1m[34m$file_logger[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[11;12H green [1m[31m"[m[1m[35mlogger is empty[m[1m[31m"[m[12;8H [1m[37melse[m[13;12H red [1m[31m"[m[1m[35mlogger is not empty[m[1m[31m"[m[14;12H [1m[36mline[m=[35m`cat [m[1m[34m$file_logger[m[35m`[m[15;12H print_error [1m[31m"[m[1m[34m$line[m[1m[31m"[m[16;12H [36m#   use_error "$line"[m[17;8H [1m[37mfi[m
    [1m[37melse[m[19;8H red [1m[31m'[m[1m[35mno such file[m[1m[31m'[m
    [1m[37mfi[m[22;70H[1m[7m78 [9;5H[?12l[?25h[?25l[27m[m[H[2J[1;1H[1m wrapper.sh [m[7m                                                                    [m[2;4H [1m[33mexit[m[4;4H [1m[33mprint[m[1m[35m press any Key[m
    [1m[33mread[m
    [1m[36mstr1[m=[35m`cat [m[1m[34m$file_logger[m[35m [m[1m[31m|[m[35m cut -d[m[1m[31m'[m[1m[35m:[m[1m[31m'[m[35m -f2`[m
    [1m[36mstr2[m=[35m`cat [m[1m[34m$file_logger[m[35m [m[1m[31m|[m[35m cut -d[m[1m[31m'[m[1m[35m:[m[1m[31m'[m[35m -f3 [m[1m[31m|[m[35m [m[1m[33msed[m[35m [m[1m[31m'[m[1m[35ms/line /+/g[m[1m[31m'[m[35m`[m
    [1m[36mstr3[m=[35m`cat [m[1m[34m$file_logger[m[35m [m[1m[31m|[m[35m cut -d[m[1m[31m'[m[1m[35m:[m[1m[31m'[m[35m -f4`[m
    [1m[36mstr4[m=[35m`cat [m[1m[34m$file_logger[m[35m [m[1m[31m|[m[35m cut -d[m[1m[31m'[m[1m[35m:[m[1m[31m'[m[35m -f5`[m
