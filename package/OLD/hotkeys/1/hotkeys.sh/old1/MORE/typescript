    [1m[34mIFS[m[1m[31m=[m[1m[34m$delimeter[m [1m[33mread[m [35m-a[m arr [1m[31m<<<[m [1m[31m"[m[1m[34m$str[m[1m[31m"[m
    [36m#result: arr[m
[1m[37m}[m[34;70H[1m[7m47 [m[35;1H[K[35;1H/end[?12l[?25h[?25l[26;9H[30m[43mend[?12l[?25h[?25l[35;1H[27m[m[H[2J[1;1H[1m ./wrapper.sh [m[7m                                                                                                                [m[2;4H [1m[33mlocal[m [1m[36mstr[m=[35m`echo [m[1m[34m$line[m[35m [m[1m[31m|[m[35m [m[1m[33msed[m[35m [m[1m[31m'[m[1m[35ms/line /+/g[m[1m[31m'[m[35m`[m
    [36m#    echo "$str"[m
    str_to_arr [1m[31m"[m[1m[34m$str[m[1m[31m"[m
    [36m#we have: arr[m
    [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mARR[m[1m[31m"[m[8;4H [1m[36mexe[m=[1m[31m"[m[1m[34m${arr[[m[1m[35m0[m[1m[34m]}[m[1m[31m"[m
