str_to_arr(){[m
    [36m#depend on: arr[m
    [1m[33mlocal[m [1m[36mstr[m=[1m[31m"[m[1m[34m$1[m[1m[31m"[m
    [1m[33mlocal[m [1m[36mdelimeter[m=[1m[34m${2[m[1m[31m-'[m[1m[35m:[m[1m[31m'[m[1m[34m}[m
    [1m[34mIFS[m[1m[31m=[m[1m[34m$delimeter[m [1m[33mread[m [35m-a[m arr [1m[31m<<<[m [1m[31m"[m[1m[34m$str[m[1m[31m"[m
    [36m#result: arr[m
[1m[37m}


use_error(){[m
    [1m[36marr[m=()
    [1m[36mstr[m=[1m[31m"[m[1m[34m$@[m[1m[31m"[m
    [1m[34m$([m[35mstr_to_arr [m[1m[31m"[m[1m[34m$str[m[1m[31m"[m[35m [m[1m[34m)[m
    [1m[33mecho[m[1m[35m first array element:[m
    [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[34m${array[[m[1m[35m2[m[1m[34m]}[m[1m[31m"[m
    [1m[33mexit[m[23;1H[K[22;70H[1m[7m42 [9;5H[?12l[?25h[?25l[2;21r[m[2;1H[18M[1;24r[4;4H [1m[33mprint[m[1m[35m press any Key[m
    [1m[33mread[m
    [1m[36mstr1[m=[35m`cat [m[1m[34m$file_logger[m[35m [m[1m[31m|[m[35m cut -d[m[1m[31m'[m[1m[35m:[m[1m[31m'[m[35m -f2`[m
    [1m[36mstr2[m=[35m`cat [m[1m[34m$file_logger[m[35m [m[1m[31m|[m[35m cut -d[m[1m[31m'[m[1m[35m:[m[1m[31m'[m[35m -f3 [m[1m[31m|[m[35m [m[1m[33msed[m[35m [m[1m[31m'[m[1m[35ms/line /+/g[m[1m[31m'[m[35m`[m
    [1m[36mstr3[m=[35m`cat [m[1m[34m$file_logger[m[35m [m[1m[31m|[m[35m cut -d[m[1m[31m'[m[1m[35m:[m[1m[31m'[m[35m -f4`[m
    [1m[36mstr4[m=[35m`cat [m[1m[34m$file_logger[m[35m [m[1m[31m|[m[35m cut -d[m[1m[31m'[m[1m[35m:[m[1m[31m'[m[35m -f5`[m
    [1m[33mecho[m[1m[35m notify-send [m[1m[31m"[m[1m[34m$str4[m[1m[31m"[m[1m[35m [m[1m[31m"[m[1m[34m$str3[m[1m[31m"[m[1m[35m [m
    [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mvi [m[1m[34m$str2[m[1m[35m [m[1m[34m$str1[m[1m[31m"[m[1m[35m [m[1m[31m|[m xsel [35m--clipboard[m
    [1m[33msleep[m [1m[35m1[m
    [1m[33mecho[m[1m[35m [m[1m[31m'[m[1m[35myour clipboard has been updated![m[1m[31m'[m
[1m[37m}
run(){[m
    blue [1m[31m"[m[1m[35mrun()[m[1m[31m"[m
    [1m[33meval[m [1m[31m"[m[1m[34m$script[m[1m[31m"[m [1m[35m2[m[1m[31m>[m[1m[34m$file_logger[m[19;4H [1m[33mecho[m[1m[35m continue[m
