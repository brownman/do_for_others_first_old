    [1m[36mline[m=[1m[31m"[m[1m[34m${arr[[m[1m[35m1[m[1m[34m]}[m[1m[31m"[m
    [1m[30m[43merr[m[1m[36mor[m=[1m[31m"[m[1m[34m${arr[[m[1m[35m2[m[1m[34m]}[m[1m[31m"[m
    [1m[30m[43merr[m[1m[36mor_code[m=[1m[31m"[m[1m[34m${arr[[m[1m[35m3[m[1m[34m]}[m[1m[31m"[m[21;4H [36m#IFS=\s[m
    [36m#echo "${arr[@]}" > /tmp/[m[30m[43merr[m[36mor.txt[m
    notify-send [1m[31m"[m[1m[34m$[m[1m[30m[43merr[m[1m[34mor[m[1m[31m"[m [1m[31m"[m[1m[34m$[m[1m[30m[43merr[m[1m[34mor_code[m[1m[31m"[m
    [1m[33mprintf[m [1m[31m"[m[1m[35m%s[m[35m\n[m[1m[31m"[m [1m[31m"[m[1m[34m${arr[[m@[1m[34m]}[m[1m[31m"[m [1m[31m>[m /tmp/[30m[43merr[mor.txt[27;4H [36m#for debugging purpose:[m
    [36m#cat -n /tmp/[m[30m[43merr[m[36mor.txt[m[30;4H [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mvi [m[1m[34m$exe[m[1m[35m [m[1m[34m$line[m[1m[31m"[m[1m[35m [m[1m[31m|[m xsel [35m--clipboard[m
    [1m[33msleep[m [1m[35m1[m
