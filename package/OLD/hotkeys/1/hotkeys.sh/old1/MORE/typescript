    [1m[37mif [m[1m[31m[[m [1m[37m![m [1m[37m-s[m [1m[31m"[m[1m[34m$file_logger[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[19;8H green [1m[31m"[m[1m[35mlogger is empty[m[1m[31m"[m
    [1m[37melse[m[21;8H red [1m[31m"[m[1m[35mlogger is not empty[m[1m[31m"[m[22;70H[1m[7m62 [9;1H[?12l[?25h[?25l[2;21r[m[2;1H[18M[1;24r[4;8H [1m[36mline[m=[35m`cat [m[1m[34m$file_logger[m[35m`[m[5;8H print_error [1m[31m"[m[1m[34m$line[m[1m[31m"[m
