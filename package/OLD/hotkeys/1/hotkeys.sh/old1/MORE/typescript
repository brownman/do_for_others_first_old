    [1m[33mlocal[m [1m[36mline[m=[1m[31m"[m[1m[34m$1[m[1m[31m"[m
    [1m[36marr[m=()
    [1m[33mlocal[m [1m[36mstr[m=[35m`echo [m[1m[34m$line[m[35m [m[1m[31m|[m[35m [m[1m[33msed[m[35m [m[1m[31m'[m[1m[35ms/line /+/g[m[1m[31m'[m[35m`[m
[36m#    echo "$str"[m
    str_to_arr [1m[31m"[m[1m[34m$str[m[1m[31m"[m
    [36m#we have: arr[m
[1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mARR[m[1m[31m"[m
[1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[34m${arr[[m[1m[35m0[m[1m[34m]}[m[1m[31m"[m
    [36m#    echo "vi $str2 $str1" | xsel --clipboard[m
    [36m#    sleep 1[m
    [36m#    echo 'your clipboard has been updated!'[m
[1m[37m}
try(){[m
    blue [1m[31m'[m[1m[35mtry()[m[1m[31m'[m
    [36m#echo something >> $file_logger
#    $( exec "$script" 2>&1 1>$file_logger )[m
[1m[33meval[m [1m[34m$script[m [1m[35m2[m[1m[31m>[m[1m[34m$file_logger[m
green [1m[31m'[m[1m[35m--end of script --[m[1m[31m'[m
[1m[37m}
