    [1m[36marr[m=()
    [1m[36mstr[m=[1m[31m"[m[1m[34m$@[m[1m[31m"[m
    [1m[34m$([m[35mstr_to_arr [m[1m[31m"[m[1m[34m$str[m[1m[31m"[m[35m [m[1m[34m)[m[23;1H[K[22;71H[1m[7m6[9;1H[?12l[?25h[22;71H7 [10;1H[m [?25l[27m[m[H[2J[1;1H[1m wrapper.sh [m[7m                                                                    [m[2;1H[36m#!/bin/bash -e[m

