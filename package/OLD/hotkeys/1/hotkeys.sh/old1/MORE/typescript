blue()       {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;30m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}
red() {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;31m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}
green()         {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;32m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}

print_error(){[m
    red [1m[31m"[m[1m[35mprint error:[m[1m[31m"[m
    [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[34m$@[m[1m[31m"[m
[1m[37m}
str_to_arr(){[m
    [36m#depend on: arr[m
    [36m#blue 'str_to_arr'[m
    [1m[33mlocal[m [1m[36mstr[m=[1m[31m"[m[1m[34m$1[m[1m[31m"[m
    [1m[33mlocal[m [1m[36mdelimeter[m=[1m[34m${2[m[1m[31m-[m:[1m[34m}[m
    [1m[34mIFS[m[1m[31m=[m[1m[34m$delimeter[m [1m[33mread[m [35m-a[m arr [1m[31m<<<[m [1m[31m"[m[1m[34m$str[m[1m[31m"[m
    [36m#result: arr[m
[1m[37m}


use_error(){[m
    blue [1m[31m'[m[1m[35muse_error()[m[1m[31m'[m
    [1m[33mlocal[m [1m[36mline[m=[1m[31m"[m[1m[34m$1[m[1m[31m"[m
    [1m[36marr[m=()
    [1m[33mlocal[m [1m[36mstr[m=[35m`echo [m[1m[34m$line[m[35m [m[1m[31m|[m[35m [m[1m[33msed[m[35m [m[1m[31m'[m[1m[35ms/line /+/g[m[1m[31m'[m[35m`[m
    [36m#    echo "$str"[m
    str_to_arr [1m[31m"[m[1m[34m$str[m[1m[31m"[m
    [36m#we have: arr[m
    [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mARR[m[1m[31m"[m
