    [1m[37mif [m[1m[31m[[m [1m[37m-f[m [1m[31m"[m[1m[34m$script[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[13;8H [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mscript exist[m[1m[31m"[m[15;4H [1m[37melse[m[16;8H [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mno such script[m[1m[31m"[m[17;8H [1m[33mexit[m
