[1m[33m        local s[m[1m[7m[33mtr[m[1m[33m1[m=[1m[35m"${BASH_SOURCE[$i]}"[m
[1m[33m        local str2[m=[1m[35m"${BASH_LINENO[$i-1]}"[m
[1m[33m        local str3[m=[1m[35m"${FUNCNAME[$i]}"[m

[1m[33m        msg[m=[1m[35m"$msg\n$str1:$str2:$str3" [m[17;4H done[19;4H echo -e "$msg">> $file_error
    if [1m[32m[ "$msg" ][m[36m;then[m
[1m[33m        local  cmd[m=[1m[35m"gxmessage -file $file_error -title 'breakpoint: $msg'"[m
[1m[7m <AIL/suspend.sh/cfg/funcs.cfg   CWD: /home/dao04/JAIL/suspend.sh/cfg   Line: 89[m[23;1H/tr[?12l[?25ha[?25l[27m[m[H[2J[1;1H[1m funcs.cfg [m[7m                                                                     [m[2;1H[1m[33m    why[m=[1m[35m"$2"[m
    print_error "Reason Of Death:"
    echo -e "\twho:  $who:"
    echo -e "\twhy:  $why"
    exiting
}

