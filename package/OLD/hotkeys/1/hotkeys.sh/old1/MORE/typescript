    [1m[36mscript[m=[1m[34m$1[m
    [1m[37mif [m[1m[31m[[m [1m[31m"[m[1m[34m$2[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[8;8H [1m[33mshift[m[22;70H[1m[7m9  [10;5H[?12l[?25h[?25l[?12l[?25h[22;70H10[11;4H[22;71H1 [12;5H[22;71H2 [13;5H[22;71H3 [14;2H[?25l[2;21r[m[21;1H
[1;24r[21;4H red [1m[31m"[m[1m[35mprint error:[m[1m[31m"[m[22;71H[1m[7m4 [14;1H[?12l[?25h[?25l[2;21r[m[21;1H
[1;24r[21;4H [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[34m$@[m[1m[31m"[m[22;71H[1m[7m5 [14;1H[?12l[?25h[?25l[2;21r[m[21;1H
