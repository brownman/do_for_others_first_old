notify-send [1m[31m"[m[1m[35mdebug or undebug ?[m[1m[31m"[m
[1m[33mexport[m[1m[36m file_logger=[m/tmp/logger.txt

[36m#echo -n '' > $file_logger[m
[1m[37mif [m[1m[31m[[m [1m[37m-f[m [1m[31m"[m[1m[34m$file_logger[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m
