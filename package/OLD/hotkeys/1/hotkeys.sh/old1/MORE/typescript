    [1m[33mecho[m[1m[35m [m[1m[31m'[m[1m[35msupply a script to wrap[m[1m[31m'[m
    [1m[33mexit[m
[1m[37mfi


native()       {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;30m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}
function[m [1m[37mred {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;31m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}
green()         {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;32m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}

print_error(){[m
[1m[7m ~/JAIL/pilot.sh/wrapper.sh   CWD: /home/dao04/JAIL/pilot.sh   Line: 1          [2;1H[?12l[?25h[?25l[2;21r[m[2;1H[18M[1;24r[4;4H red [1m[31m"[m[1m[35mprint error:[m[1m[31m"[m
    [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[34m$@[m[1m[31m"[m
[1m[37m}
str_to_arr(){[m
    [36m#depend on: arr[m
    [1m[33mlocal[m [1m[36mstr[m=[1m[31m"[m[1m[34m$1[m[1m[31m"[m
    [1m[33mlocal[m [1m[36mdelimeter[m=[1m[34m${2[m[1m[31m-'[m[1m[35m:[m[1m[31m'[m[1m[34m}[m
    [1m[34mIFS[m[1m[31m=[m[1m[34m$delimeter[m [1m[33mread[m [35m-a[m arr [1m[31m<<<[m [1m[31m"[m[1m[34m$str[m[1m[31m"[m
    [36m#result: arr[m
[1m[37m}


use_error(){[m
    [1m[36marr[m=()
    [1m[36mstr[m=[1m[31m"[m[1m[34m$@[m[1m[31m"[m
    [1m[34m$([m[35mstr_to_arr [m[1m[31m"[m[1m[34m$str[m[1m[31m"[m[35m [m[1m[34m)[m
    [1m[33mecho[m[1m[35m first array element:[m
