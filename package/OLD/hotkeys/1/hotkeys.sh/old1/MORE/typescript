    red [1m[31m"[m[1m[35mprint [m[1m[30m[43merr[m[1m[35mor:[m[1m[31m"[m
    [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[34m$@[m[1m[31m"[m
[1m[37m}
str_to_arr(){[m
    [36m#depend on: arr[m
    [36m#blue 'str_to_arr'[m
    [1m[33mlocal[m [1m[36mstr[m=[1m[31m"[m[1m[34m$1[m[1m[31m"[m
    [1m[33mlocal[m [1m[36mdelimeter[m=[1m[34m${2[m[1m[31m-[m:[1m[34m}[m
    [1m[34mIFS[m[1m[31m=[m[1m[34m$delimeter[m [1m[33mread[m [35m-a[m arr [1m[31m<<<[m [1m[31m"[m[1m[34m$str[m[1m[31m"[m
