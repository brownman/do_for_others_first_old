    [14C [4C [2C arr [3C 
    [36m#result: arr[m
[1m[37m}[11;11H[?12l[?25h[?25l[m[1m[37m[46m()[m[1m[37m {[m [?12l[?25h[36;1H[K[11;12H[?25l[?12l[?25h[?25l[35;1H:[?12l[?25hw[?25l[?12l[?25h[?25l"wrapper.sh" 147L, 2813C written[1;2H[1mwrapper.shs h[m[7m  [m[34;28H[1m[7m   CWD: /home/dao04/JAIL/error.sh   Line: 40    [11;12H[?12l[?25h[?25l[2;33r[m[2;1H[30M[1;36r[4;1H[1m[37muse_[m[1m[30m[43merr[m[1m[37mor(){[m
    blue [1m[31m'[m[1m[35muse_[m[1m[30m[43merr[m[1m[35mor()[m[1m[31m'[m
    [1m[33mlocal[m [1m[36mline[m=[1m[31m"[m[1m[34m$1[m[1m[31m"[m
    [1m[36marr[m=()
    [1m[33mlocal[m [1m[36mstr[m=[35m`echo [m[1m[34m$line[m[35m [m[1m[31m|[m[35m [m[1m[33msed[m[35m [m[1m[31m'[m[1m[35ms/line /+/g[m[1m[31m'[m[35m`[m
