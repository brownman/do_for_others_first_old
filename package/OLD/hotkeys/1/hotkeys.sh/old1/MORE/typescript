show_log(){[m
    blue  [1m[31m'[m[1m[35mshow_log()[m[1m[31m'[m [36m#[m
    [36m#    exit[m
    [36m#    cmd="$script ${args[@]}" [m
    [36m#    echo "cmd: $cmd"[m[23;4H [36m#    $(eval "$cmd") 2> $file_logger [m
    [36m#[m
    [1m[37mif [m[1m[31m[[m [1m[37m-f[m [1m[31m"[m[1m[34m$file_logger[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[26;8H [1m[37mif [m[1m[31m[[m [1m[37m-s[m [1m[31m"[m[1m[34m$file_logger[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[27;12H red [1m[31m"[m[1m[35mlogger is not empty[m[1m[31m"[m[28;12H [1m[33mecho[m[1m[35m Press Enter![m[29;12H [1m[33mread[m[30;12H [1m[36mline[m=[35m`cat [m[1m[34m$file_logger[m[35m`[m[31;12H print_[30m[43merr[mor [1m[31m"[m[1m[34m$line[m[1m[31m"[m[32;12H use_[30m[43merr[mor [1m[31m"[m[1m[34m$line[m[1m[31m"[m[33;12H [36m#   use_[m[30m[43merr[m[36mor "$line"[m[34;70H[1m[7m98[9;5H[?12l[?25h[34;71H9 [10;5H[34;70H100[11;5H[34;70H99  [10;5H[?25l[m[36;1H[1m-- INSERT --[10;5H[?12l[?25h[1;2H+r awrapper.sh [?25l[m[10;4H uif [1m[31m[[m [1m[31m"[m[1m[34m$[m[1m[30m[43merr[m[1m[34mor_code[m[1m[31m"[m [1m[37m-eq[m [1m[35m0[m   [1m[31m];[mthen[12;5Helse[14;5Hfi[34;28H[1m[7m[+]   CWD: /home/dao04/JAIL/error.sh   Line: 99[10;6H[?12l[?25h[?25l[m [1m[37mif [m[1m[31m[[m [1m[31m"[m[1m[34m$[m[1m[30m[43merr[m[1m[34mor_code[m[1m[31m"[m [1m[37m-eq[m [1m[35m0[m   [1m[31m];[m[1m[37mthen[m[10;38H[K[12;5H[1m[37melse[14;5Hfi[10;5H[?12l[?25h[m[36;1H[K[10;4H[?25l[?12l[?25h[?25l[36;1H[1m-- INSERT --[11;33r[m[11;1H[L[1;36r[36;1H[K[36;1H[1m-- INSERT --[m[34;73H[1m[7m100[11;9H[?12l[?25h[?25l[m i[?12l[?25h[?25l[11;9H[K[11;9H[?12l[?25h[?25l o[?12l[?25h[?25l[11;9H[K[11;9H[?12l[?25h[?25l p[?12l[?25h[?25lpr[?12l[?25h[?25lri[?12l[?25h[?25lin[?12l[?25h[?25l[1m[33mprint[?12l[?25h[?25l[mprint_[?12l[?25h[?25l_l[?12l[?25h[?25lli[?12l[?25h[?25lin[?12l[?25h[?25lne[?12l[?25h[?25l [?12l[?25h[?25l[?12l[?25h[?25l[11;18H[K[11;18H[?12l[?25h[?25l[11;17H[K[11;17H[?12l[?25h[?25l