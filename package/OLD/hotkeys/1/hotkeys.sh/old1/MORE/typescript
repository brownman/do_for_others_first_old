    red [1m[33mprint[m[1m[35m error:[m
    [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[34m$@[m[1m[31m"[m
[1m[37m}
str_to_arr(){[m
    [36m#depend on: arr[m
    [1m[33mlocal[m [1m[36mstr[m=[1m[31m"[m[1m[34m$1[m[1m[31m"[m
    [1m[33mlocal[m [1m[36mdelimeter[m=[1m[34m${2[m[1m[31m-'[m[1m[35m:[m[1m[31m'[m[1m[34m}[m
    [1m[34mIFS[m[1m[31m=[m[1m[34m$delimeter[m [1m[33mread[m [35m-a[m arr [1m[31m<<<[m [1m[31m"[m[1m[34m$str[m[1m[31m"[m
    [36m#result: arr[m
[1m[37m}[m


[1m[7m ~/JAIL/pilot.sh/wrapper.sh   CWD: /home/dao04/JAIL/pilot.sh   Line: 1          [2;1H[?12l[?25h[22;70H2 [3;1H[22;70H3 [4;1H[22;70H4 [5;1H[22;70H5 [6;1H[22;70H4 [5;1H[22;70H3 [4;1H[22;70H2 [3;1H[22;70H3 [4;1H[22;70H4 [5;1H[22;70H5 [6;1H[22;70H6 [7;1H[22;70H7 [8;1H[?25l[2;21r[m[2;1H[18M[1;24r[4;1H[1m[37muse_error(){[m
