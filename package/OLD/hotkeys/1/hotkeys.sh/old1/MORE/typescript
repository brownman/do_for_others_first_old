    [1m[30m[43merr[m[1m[36mor_code[m=[1m[31m"[m[1m[34m${arr[[m[1m[35m3[m[1m[34m]}[m[1m[31m"[m[17;4H [36m#IFS=\s[m
    [36m#echo "${arr[@]}" > /tmp/[m[30m[43merr[m[36mor.txt[m
    notify-send [1m[31m"[m[1m[34m$[m[1m[30m[43merr[m[1m[34mor[m[1m[31m"[m [1m[31m"[m[1m[34m$[m[1m[30m[43merr[m[1m[34mor_code[m[1m[31m"[m
    [1m[33mprintf[m [1m[31m"[m[1m[35m%s[m[35m\n[m[1m[31m"[m [1m[31m"[m[1m[34m${arr[[m@[1m[34m]}[m[1m[31m"[m [1m[31m>[m /tmp/[30m[43merr[mor.txt[23;4H [36m#for debugging purpose:[m
    [36m#cat -n /tmp/[m[30m[43merr[m[36mor.txt[m[26;4H [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mvi [m[1m[34m$exe[m[1m[35m [m[1m[34m$line[m[1m[31m"[m[1m[35m [m[1m[31m|[m xsel [35m--clipboard[m
    [1m[33msleep[m [1m[35m1[m
    green [1m[31m'[m[1m[35myour clipboard has been updated![m[1m[31m'[m
[1m[37m}
try(){[m
    blue [1m[31m'[m[1m[35mtry()[m[1m[31m'[m
    [36m#echo something >> $file_logger[m
    [36m#    $( exec "$script" 2>&1 1>$file_logger )[m[34;70H[1m[7m68[9;1H[?12l[?25h[?25l[27m[m[H[2J[1;1H[1m wrapper.sh [m[7m                                                                                                                  [m[2;4H [1m[37mif [m[1m[31m[[m [1m[31m"[m[1m[34m$2[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[3;8H [1m[33mshift[m[4;8H [1m[36margs[m=( [1m[31m"[m[1m[34m$@[m[1m[31m"[m [1m[37m[41m)[m
    [1m[37mfi
else[m
    [1m[33mecho[m[1m[35m [m[1m[31m'[m[1m[35msupply a script to wrap[m[1m[31m'[m
    [1m[33mexit[m
[1m[37mfi


blue()       {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;30m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}
red() {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;31m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}
green()         {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;32m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}

