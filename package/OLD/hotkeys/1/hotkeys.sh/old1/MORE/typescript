    [1m[36mscript[m=[1m[31m"[m[1m[34m$path[m[1m[35m/[m[1m[34m$1[m[1m[31m"[m[22;4H [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mscript: [m[1m[34m$script[m[1m[31m"[m
