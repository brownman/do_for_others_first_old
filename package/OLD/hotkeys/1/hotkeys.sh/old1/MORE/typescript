str_to_arr(){[m[22;70H[1m[7m26[9;5H[?12l[?25h[?25l[2;21r[m[2;1H[18M[1;24r[3;13H[1m[37m[46m{[m
    [36m#depend on: arr[m
    [1m[33mlocal[m [1m[36mstr[m=[1m[31m"[m[1m[34m$1[m[1m[31m"[m
    [1m[33mlocal[m [1m[36mdelimeter[m=[1m[34m${2[m[1m[31m-'[m[1m[35m:[m[1m[31m'[m[1m[34m}[m
    [1m[34mIFS[m[1m[31m=[m[1m[34m$delimeter[m [1m[33mread[m [35m-a[m arr [1m[31m<<<[m [1m[31m"[m[1m[34m$str[m[1m[31m"[m
    [36m#result: arr[m
[1m[37m[46m}[m


[1m[37muse_error(){[m
    [1m[36marr[m=()
    [1m[36mstr[m=[1m[31m"[m[1m[34m$@[m[1m[31m"[m
    [1m[36mstr[m=[35m`cat [m[1m[34m$file_logger[m[35m [m[1m[31m|[m[35m [m[1m[33msed[m[35m [m[1m[31m'[m[1m[35ms/line /+/g[m[1m[31m'[m[35m`[m[17;4H [1m[34m$([m[35mstr_to_arr [m[1m[31m"[m[1m[34m$str[m[1m[31m"[m[35m [m[1m[34m)[m


[36m#    echo notify-send "$arrstr4" "$str3" 
#    echo "vi $str2 $str1" | xsel --clipboard[m[22;70H[1m[7m44 [9;1H[?12l[?25h[?25l[2;21r[m[2;1H[18M[1;24r[4;1H[36m#    sleep 1
#    echo 'your clipboard has been updated!'[m
[1m[37m}
try(){[m
    blue [1m[31m'[m[1m[35mtry()[m[1m[31m'[m
[1m[33mecho[m[1m[35m something [m[1m[31m>>[m [1m[34m$file_logger[m
[1m[33meval[m [1m[31m"[m[1m[34m$script[m[1m[31m"[m [1m[35m2[m[1m[31m>&1[m [1m[35m1[m [1m[31m>[m [1m[34m$file_logger[m
cat [1m[34m$file_logger[m
[1m[37m}
show_log(){[m
    blue  [1m[31m'[m[1m[35mshow_log()[m[1m[31m'[m [36m#[m
    [36m#    exit[m
    [36m#    cmd="$script ${args[@]}" [m
    [36m#    echo "cmd: $cmd"[m
    [36m#    echo -n '' > $file_logger[m
    [36m#    $(eval "$cmd") 2> $file_logger [m
    [36m#[m
    [1m[37mif [m[1m[31m[[m [1m[37m-f[m [1m[31m"[m[1m[34m$file_logger[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[22;70H[1m[7m62 [9;1H[?12l[?25h[?25l[2;21r[m[2;1H[18M[1;24r[4;8H [1m[37mif [m[1m[31m[[m [1m[37m![m [1m[37m-s[m [1m[31m"[m[1m[34m$file_logger[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[5;12H green [1m[31m"[m[1m[35mlogger is empty[m[1m[31m"[m[6;8H [1m[37melse[m[7;12H red [1m[31m"[m[1m[35mlogger is not empty[m[1m[31m"[m[8;12H [1m[36mline[m=[35m`cat [m[1m[34m$file_logger[m[35m`[m[9;12H print_error [1m[31m"[m[1m[34m$line[m[1m[31m"[m[10;12H [36m#   use_error "$line"[m[11;8H [1m[37mfi[m
    [1m[37melse[m[13;8H red [1m[31m'[m[1m[35mno such file[m[1m[31m'[m
    [1m[37mfi

}
steps(){[m
    green [1m[31m"[m[1m[35mrun: try, show_log[m[1m[31m"[m
    blue [1m[31m"[m[1m[35mrun()[m[1m[31m"[m
try
show_log[22;70H[1m[7m80 [9;13H[?12l[?25h[?25l[2;21r[m[2;1H[18M[1;24r[5;1H[1m[37m}
