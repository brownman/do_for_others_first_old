show_log(){
    blue  'show_log()' #
    #    exit
    #    cmd=[m[1m[31m"[m[1m[34m$script[m [1m[34m${args[[m@[1m[34m]}[m[1m[31m"[m[1m[35m 
    #    echo [m[1m[31m"[mcmd: [1m[34m$cmd[m[1m[31m"[m

[1m[35m    #    [m[1m[34m$([m[1m[33meval[m[35m [m[1m[31m"[m[1m[34m$cmd[m[1m[31m"[m[1m[34m)[m[1m[35m 2> [m[1m[34m$file_logger[m[1m[35m 
    #
    if [ -f [14C ];then
        if [ -s [14C ];then[18;26H[?12l[?25h[?25l[19;33r[m[19;1H[L[1;36r[18;26H[K[19;4H error_code: [1m[34m$error_code[m[1m[31m"[m[36;1H[K[36;1H[1m-- INSERT --[m[34;74H[1m[7m1 [19;5H[?12l[?25h[m[36;1H[K[19;4H[?25l[?12l[?25h[?25l[19;33r[33;1H
[1;36r[19;1H    [1m[37melse[m
    red [1m[31m"[m[1m[35merror_code: [m
    [1m[37mfi

}
