[1m[7m ~/JAIL/pilot.sh/wrapper.sh   CWD: /home/dao04/JAIL/pilot.sh   Line: 58         [11;6H[?12l[?25hP+q436f\P+q6b75\P+q6b64\P+q6b72\P+q6b6c\P+q2332\P+q2334\P+q2569\P+q2a37\P+q6b31\[?25l[27m[m[H[2J[1;1H[1m wrapper.sh [m[7m                                                                    [m[2;1H[36m#!/bin/bash -e[m
[1m[36mfile_logger[m=/tmp/logger.txt

[1m[37mif [m[1m[31m[[m [1m[31m"[m[1m[34m$1[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m
    [1m[36mscript[m=[1m[34m$1[m
    [1m[37mif [m[1m[31m[[m [1m[31m"[m[1m[34m$2[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[8;8H [1m[33mshift[m[9;8H [1m[36margs[m=( [1m[31m"[m[1m[34m$@[m[1m[31m"[m [1m[37m[41m)[m
    [1m[37mfi
