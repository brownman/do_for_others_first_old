    [1m[37mif [m[1m[31m[[m [1m[37m-f[m [1m[31m"[m[1m[34m$script[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[21;8H [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mscript exist, continue...[m[1m[31m"[m[23;4H [1m[37melse[m[24;8H [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mno such script[m[1m[31m"[m[25;8H [1m[33mexit[m
    [1m[37mfi[m[29;4H [1m[37mif [m[1m[31m[[m [1m[31m"[m[1m[34m$2[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[30;8H [1m[33mshift[m[31;8H [1m[36margs[m=( [1m[31m"[m[1m[34m$@[m[1m[31m"[m [1m[37m[41m)[m
