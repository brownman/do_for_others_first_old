str_to_arr(){[m
    [36m#depend on: arr[m
    [1m[33mlocal[m [1m[36mstr[m=[1m[31m"[m[1m[34m$1[m[1m[31m"[m
    [1m[33mlocal[m [1m[36mdelimeter[m=[1m[34m${2[m[1m[31m-'[m[1m[35m:[m[1m[31m'[m[1m[34m}[m
[1m[7m ~/JAIL/pilot.sh/wrapper.sh   CWD: /home/dao04/JAIL/pilot.sh   Line: 34         [14;1H[?12l[?25h[?25l[27m[m[H[2J[1;1H[1m wrapper.sh [m[7m                                                                    [m[2;1H[1m[33mexport[m[1m[36m file_logger=[m/tmp/logger.txt
[1m[33mexport[m[1m[36m path=[m[35m`[m[1m[33mpwd[m[35m`[m

[1m[37mif [m[1m[31m[[m [1m[31m"[m[1m[34m$1[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m
    [1m[36mscript[m=[1m[31m"[m[1m[34m$path[m[1m[35m/[m[1m[34m$1[m[1m[31m"[m[8;4H [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mscript: [m[1m[34m$script[m[1m[31m"[m
    [1m[37mif [m[1m[31m[[m [1m[37m-f[m [1m[31m"[m[1m[34m$script[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[10;8H [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mscript exist, continue...[m[1m[31m"[m[12;4H [1m[37melse[m[13;8H [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[35mno such script[m[1m[31m"[m[14;8H [1m[33mexit[m
    [1m[37mfi[m[18;4H [1m[37mif [m[1m[31m[[m [1m[31m"[m[1m[34m$2[m[1m[31m"[m [1m[31m];[m[1m[37mthen[m[19;8H [1m[33mshift[m[20;8H [1m[36margs[m=( [1m[31m"[m[1m[34m$@[m[1m[31m"[m [1m[37m[41m)[m
    [1m[37mfi[m
[1m[7m ~/JAIL/pilot.sh/wrapper.sh   CWD: /home/dao04/JAIL/pilot.sh   Line: 16         [14;9H[?12l[?25h[?25l[2;21r[m[2;1H[3L[1;24r[2;1H[36m#!/bin/bash[m

