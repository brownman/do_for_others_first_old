[1m[37mfi


blue()       {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;30m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}
red() {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;31m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}
green()         {[m [1m[33mecho[m[1m[35m -e [m[1m[31m"[m[35m\x1B[m[1m[35m[01;32m[*][m[35m\x1B[m[1m[35m[0m [m[1m[34m$@[m[1m[31m";[m [1m[37m}

print_error(){[m
    red [1m[31m"[m[1m[35mprint error:[m[1m[31m"[m
    [1m[33mecho[m[1m[35m [m[1m[31m"[m[1m[34m$@[m[1m[31m"[m
[1m[37m}
str_to_arr(){[m[22;70H[1m[7m26[9;5H[?12l[?25h[22;71H7 [10;2H[22;71H8 [11;1H[22;71H9 [12;1H[?25l[m[24;1H[1m-- VISUAL --[12;1H[?12l[?25h[?25l[m[7m [m
[1m[7m[37mblue[m[1m[37m[46m()[m[1m[37m       {[m [22;70H[1m[7m30 [13;5H[?12l[?25h[?25l[m[1m[7m[37m()       {[m[7m [m[1m[7m[33mecho[m[1m[7m[35m -e [m[1m[7m[31m"[m[7m[35m\x1B[m[1m[7m[35m[01;30m[*][m[7m[35m\x1B[m[1m[7m[35m[0m [m[1m[7m[34m$@[m[1m[7m[31m";[m[7m [m[1m[7m[37m}[m[7m [m
[1m[7m[37mred([m[1m[37m[46m)[m[1m[37m {[m [22;71H[1m[7m1 [14;5H[?12l[?25h[?25l[2;21r[m[21;1H
[1;24r[13;5H[1m[7m[37m) {[m[7m [m[1m[7m[33mecho[m[1m[7m[35m -e [m[1m[7m[31m"[m[7m[35m\x1B[m[1m[7m[35m[01;31m[*][m[7m[35m\x1B[m[1m[7m[35m[0m [m[1m[7m[34m$@[m[1m[7m[31m";[m[7m [m[1m[7m[37m}[m[7m [m
[1m[7m[37mgree[m[1m[37mn()         {[m [21;4H [36m#depend on: arr[m[24;1H[K[24;1H[1m-- VISUAL --[m[22;71H[1m[7m2 [14;5H[?12l[?25h[?25l[2;21r[m[21;1H
[1;24r[13;5H[1m[7m[37mn()         {[m[7m [m[1m[7m[33mecho[m[1m[7m[35m -e [m[1m[7m[31m"[m[7m[35m\x1B[m[1m[7m[35m[01;32m[*][m[7m[35m\x1B[m[1m[7m[35m[0m [m[1m[7m[34m$@[m[1m[7m[31m";[m[7m [m[1m[7m[37m}[m[7m [m[21;4H [1m[33mlocal[m [1m[36mstr[m=[1m[31m"[m[1m[34m$1[m[1m[31m"[m[24;1H[K[24;1H[1m-- VISUAL --[m[22;71H[1m[7m3 [14;1H[?12l[?25h[?25l[m[23;1H5 fewer lines[24;1H[K[24;1H[1m-- INSERT --[1;2H+r awrapper.sh [10;21r[m[10;1H[5M[1;24r[17;4H [1m[33mlocal[m [1m[36mdelimeter[m=[1m[34m${2[m[1m[31m-'[m[1m[35m:[m[1m[31m'[m[1m[34m}[m
    [1m[34mIFS[m[1m[31m=[m[1m[34m$delimeter[m [1m[33mread[m [35m-a[m arr [1m[31m<<<[m [1m[31m"[m[1m[34m$str[m[1m[31m"[m
    [36m#result: arr[m
[1m[37m}[m[22;28H[1m[7m[+]   CWD: /home/dao04/JAIL/pilot.sh   Line: 29[m[23;1H[K[24;1H[K[24;1H[1m-- INSERT --[10;1H[?12l[?25h[m[24;1H[K[10;1H[?25l[?12l[?25h[?25l[27m[m[H[2J[1;1H[1m + wrapper.sh [m[7m                                                                  [m[2;1H[36m#!/bin/bash -e[m

