#!/bin/bash -e
env
exit

echo selfish
pushd `dirname $0`
set -o  nounset
source "cfg/basics.cfg"
source "cfg/pid.cfg"
#source "cfg/basics.cfg"
#source "cfg/basics.cfg"

run(){


file="$1"

str=`cat $file|grep help -m1`
if [ -n "$str" ];then
    $file
else
    reason_of_death 'no self help' "$file"
fi

}
popd
env
