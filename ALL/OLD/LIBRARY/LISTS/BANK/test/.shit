proxy1(){
    if [ $# -gt 0 ];then
       local cmd="$1"
        
      local   args=()
        shift

        if [ $# -gt 0 ];then
            args=( "$@" )

            commander "$cmd ${args[@]}"
        else

            commander "$cmd"
        fi

    fi
}
commander(){
    if [ $# -gt 0 ];then
local     args=( "$@" )
 local    cmd="${args[@]}"
echo "$cmd" >> $file_logger
echo "_${cmd}___"
 

eval "$cmd"
else
    echo 'no command'
    fi
}
sleeper(){
        proxy1 sleep 2
        echo -ne "------->"
proxy1 sleep 2
        echo -ne "\t------->"
        #proxy1 xcowsay "$title"

}

